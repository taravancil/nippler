{
  "name": "nippler.org",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taravancil/nippler.org.git"
  },
  "author": "Tara Vancil <tbvanc@gmail.com>",
  "scripts": {
    "build": "webpack --mode production; lessc src/less/index.less dist/css/index.css --clean-css",
    "predeploy": "export NODE_ENV=production; gulp build; uglifyjs build/js/bundle.js -c -m -o build/js/bundle.js;",
    "watch": "webpack -w --mode development; less-watch-compiler",
    "start": "webpack-dev-server --open --mode development; less-watch-compiler"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taravancil/nippler.org/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "less": "^3.8.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-watch-compiler": "^1.11.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
